<meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="theme-color" content="#ffffff">
    <meta name="robots" content="index, follow">

    <link rel="canonical" href="{{ site.url }}{{ page.url }}">
    <link rel="icon" href="data:,">

    <meta name="description" content="{{ meta.description }}">
    <meta property="og:title" content="{{ title }}{{ site.separator }}{{ site.title}}" />
    <meta property="og:type" content="website" />    
    <meta property="og:url" content="{{ site.url }}{{ page.url }}" />
    <meta property="og:description" content="{{ meta.description }}" />
    <meta property="og:site_name" content="{{ site.title }}" />
    <meta property="og:locale" content="en_au" />

    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="{{ site.url }}{{ page.url }}#site">
    <meta name="twitter:title" content="{{ title }}{{ site.separator }}{{ site.title}}">
    <meta name="twitter:description" content="{{ meta.description }}">

    <meta name="generator" content="{{ eleventy.generator }}">

    <link type="text/plain" rel="author" href="{{site.url}}/humans.txt" />

    <title>{{ title }}{{ site.separator }}{{ site.title }}</title>